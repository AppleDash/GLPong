<?xml version="1.0" encoding="UTF-8" ?>
<Configuration status="ERROR">
	<Appenders>
		<Console name="CONSOLE" target="SYSTEM_OUT">
			<PatternLayout pattern="[%d{hh:mm:ss a}] %-5level %logger{1} - %msg%n" />
		</Console>
	</Appenders>

	<Loggers>
		<Root level="debug">
			<AppenderRef ref="Console" />
		</Root>

		<Root level="DEBUG">
			<AppenderRef ref="CONSOLE" />
		</Root>
	</Loggers>
</Configuration>
